# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Book {
  book_id: ID!
  book_title: String
  isbn13: Int!
  owner_id: Int!
}

input NewBook {
  book_title: String
  isbn13: Int!
  owner_id: Int!
}

type Mutation{
  createBook(input: NewBook!): Book!
}

type Query {
  books: [Book!]!
}